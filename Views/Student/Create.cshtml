@model Login.Student

@{
    Layout = null;
}

@using (Html.BeginForm("Create", "Student", FormMethod.Post, new { id = "studentForm" }))
{
    <label>Roll No:</label>
    @Html.TextBoxFor(m => m.roll_no, new { required = "required", @class = "form-control" })
    <br />
    <label>Name:</label>
    @Html.TextBoxFor(m => m.name, new { required = "required", @class = "form-control" })
    <br />
    <label>Enrollment Date:</label>
    @Html.TextBoxFor(m => m.Enrollment_date, new { required = "required", @class = "form-control" })
    <br />
    <button type="submit" class="btn btn-primary">Add</button>
}
